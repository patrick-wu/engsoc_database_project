<div class = "row">
  <div class="well col-md-4 col-md-offset-4">
    <%= form_for @club do |f| %>
      <p>
        <%= f.label :Club_Name %><br/>
        <%= f.text_field :name %>
      </p>
      <p>
        <%= f.label :Club_Category%><br/>
        <%= f.radio_button :category, 'Design'%> Design
        <%= f.radio_button :category, 'Cultural'%> Cultural
        <%= f.radio_button :category, 'Professional'%> Professional
        <%= f.radio_button :category, 'Musical'%> Musical <br/>
        <%= f.radio_button :category, 'Hobby/Interest'%> Hobby/Interest
        <%= f.radio_button :category, 'Athletic'%> Athletic
        <%= f.radio_button :category, 'Societal'%> Societal
        <%= f.radio_button :category, 'Fine Arts'%> Fine Arts <br/>
      </p>
      <p>
        <%= f.label :email%><br/>
        <%= f.text_field :email %>
      </p>
      <p>
        <%= f.label :Website_if_applicable%><br/>
        <%= f.text_field :website %>
      </p>
      <p>
        <%= f.label :Founding_Year%><br/>
        <%= f.text_field :founding_year %>
      </p>


      <%= f.submit(@club.new_record? ? 'Register' : 'Edit Profile' ,class: "btn btn-success") %>
    <% end %>
  </div>
</div>
